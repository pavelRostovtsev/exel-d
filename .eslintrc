{   
    
    "parser": "babel-eslint",
    "env": {
       "es6": true,
       "browser": true, 
       "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "linebreak-style": 0,
        "indent":"off",
        "comma-dangle":"off",
        "comma-spacing":"off",
        "comma-style":"off",
        "no-trailing-spaces":"off",
        "keyword-spacing":"off",
        "eol-last": "off",
        "require-jsdoc": "off"    
    }
   
}
